<UserControl x:Class="GraficadorSeñales.ConfiguracionSeñalSenoidal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GraficadorSeñales"
             mc:Ignorable="d" 
             d:DesignHeight="118.5" d:DesignWidth="233">
    <Grid>
        <TextBox x:Name="txtAmplitud" HorizontalAlignment="Left" Height="23" Margin="108,8,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Text="1"/>
        <TextBlock x:Name="Amplitud" HorizontalAlignment="Left" Margin="16,9,0,0" TextWrapping="Wrap" Text="Amplitud" VerticalAlignment="Top"/>
        <TextBlock x:Name="Fase" HorizontalAlignment="Left" Margin="16,51,0,0" TextWrapping="Wrap" Text="Fase" VerticalAlignment="Top"/>
        <TextBlock x:Name="Frecuencia" HorizontalAlignment="Left" Margin="16,89,0,0" TextWrapping="Wrap" Text="Frecuencia" VerticalAlignment="Top"/>
        <TextBox x:Name="txtFrecuencia" HorizontalAlignment="Left" Height="23" Margin="108,86,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Text="1"/>
        <TextBox x:Name="txtFase" HorizontalAlignment="Left" Height="23" Margin="108,47,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Text="0"/>
    </Grid>
</UserControl>
